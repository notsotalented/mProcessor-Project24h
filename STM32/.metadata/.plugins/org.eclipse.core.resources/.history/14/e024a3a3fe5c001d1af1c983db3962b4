#include "button.h"
#include "main.h"

int button_inc_flag = 0;

int KeyReg0 = NORMAL_STATE;
int KeyReg1 = NORMAL_STATE;
int KeyReg2 = NORMAL_STATE;

int TimeForKeyPress = 200;

void subKeyProcess() {
	button_inc_flag = 1;
}

void getKeyInput(){
	  KeyReg0 = KeyReg1;
	  KeyReg1 = KeyReg2;
	  KeyReg2 = HAL_GPIO_ReadPin(INC_GPIO_Port, INC_Pin);


}
